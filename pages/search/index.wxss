/* pages/search/index.wxss */
page {
  background-color: #dedede;
  padding: 20rpx;
}
.search_bar {
  height: 60rpx;
  display: flex;
}
.search_bar input {
  background-color: #fff;
  height: 100%;
  flex: 1;
  margin-right: 10rpx;
  padding-left: 30rpx;
}
.search_bar button {
width: 110rpx !important;
padding: 0;
margin: 0;
font-size: 28rpx;
height: 100%;
display: flex;
justify-content: center;
align-items: center;
}

.search_content {
  margin-top: 10rpx
}
.content_item {
  background-color: #fff;
border-bottom: 1rpx solid #dedede;
height: 60rpx;
line-height: 60rpx;
}
.content_item navigator{
white-space: nowrap;
overflow: hidden;
text-overflow: ellipsis;
}