/* 在全局样式文件中导入iconfont样式 */
@import "./style/index.wxss"

page,view,swiper,swiper-item,image,navigator {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

/* 定义主题颜色 */
page {
  --themeColor: #eb4450;
  font-size: 28rpx;
}

image {
  width: 100%;
}

.active {
  color: var(--themeColor);
  border-left: 5rpx solid currentColor;
}